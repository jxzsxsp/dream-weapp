<view class='page_container'>
    <view class="uselogos">
        <image class="logobg" src="http://cos.qkmai.com/qkmbb/myytal/wdbjs.png" style='width:100%;' lazy-load></image>
        <view class="usetitle">
            <image class="uselogo" src="{{userInfo.picture}}" lazy-load></image>
            <view class="userinfo">
                <view class="username userinfo-b">{{userInfo.realName}}</view>
                <view class="userinfo-b">
                    <view class="user-grade">{{userInfo.gradeName}}</view>
                    <view bindtap="bindPointTap" class="user-score">
                        <image src="/images/score.png" lazy-load></image>
                        <view>
                            <text>{{userInfo.points}}</text>分</view>
                    </view>
                </view>
            </view>
        </view>
        <image bindtap='goToVip' class='header-tips' style='position:absolute;bottom:0;left:0;width:750rpx;height:119rpx;' src='{{userInfo.IsReferral ? "http://cos.qkmai.com/qkmbb/myytal/wdfc2.png" : "http://cos.qkmai.com/qkmbb/myytal/wdfc1.png"}}' lazy-load></image>

        <view style='position:absolute;right:20rpx;top:60rpx;' bindtap='goToCode'>
            <image style='width:60rpx;margin-bottom:20rpx;' src='http://cos.qkmai.com/qkmbb/ytal/erweima.png' mode='widthFix'></image>
            <view style='font-size:20rpx;color:#fff;'>分销码</view>
        </view>
    </view>
    <view class="usecd">
        <view bindtap="bindStatue" class="usecdlist" data-key="1">
            <view class="iconfont icon__wallet"></view>
            <text>待付款</text>
            <view class="order_num" hidden="{{orderCount.WaitBuyerPay==0}}">
                <text>{{orderCount.WaitBuyerPay}}</text>
            </view>
        </view>
        <view bindtap="bindStatue" class="usecdlist" data-key="2">
            <view class="iconfont icon__package"></view>
            <text>拣货中</text>
            <view class="order_num" hidden="{{orderCount.BuyerAlreadyPaid==0}}">
                <text>{{orderCount.BuyerAlreadyPaid}}</text>
            </view>
        </view>
        <view bindtap="bindStatue" class="usecdlist" data-key="3">
            <view class="iconfont icon__shipping"></view>
            <text>待收货</text>
            <view class="order_num" hidden="{{orderCount.SellerAlreadySent==0}}">
                <text>{{orderCount.SellerAlreadySent}}</text>
            </view>
        </view>
        <view bindtap="bindApply" class="usecdlist">
            <view class="iconfont icon__service-1 "></view>
            <text>已取消</text>
            <view class="order_num" hidden="{{orderCount.ApplyForRefund==0}}">
                <text>{{orderCount.ApplyForRefund}}</text>
            </view>
        </view>
        <image src="http://cos.qkmai.com/qkmbb/ytal/dakuohaotb.png" class='line'></image>
        <view bindtap="bindStatue" class="usecdlist" data-key="0">
            <image src="http://cos.qkmai.com/qkmbb/ytal/dingdantb.png" class='order_icon'></image>
            <text>我的订单</text>
        </view>
    </view>
    <view class='usecdsecond'>
        <view bindtap="bindMyAddressTap" class="usecdlist">
            <image src="http://cos.qkmai.com/qkmbb/ytal/wodetb1.png" class='small_icon'></image>
            <text>地址库</text>
        </view>
        <view bindtap="bindMyCouponsTap" class="usecdlist">
            <image src="http://cos.qkmai.com/qkmbb/ytal/wodetb2.png" class='small_icon'></image>
            <text>优惠券</text>
        </view>
        <view bindtap="" class="usecdlist" style='position:;relative'>
            <image src="http://cos.qkmai.com/qkmbb/ytal/wodetb3.png" class='small_icon'></image>
            <text>咨询服务</text>
            <button openType="contact" plain='true' style='position:absolute;left:0;top:0;width:100%;height:100%;border:0;color:transparent;'>1</button>
        </view>
