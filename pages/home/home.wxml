    <view style='width:730rpx;margin-left:10rpx;padding:0;font-size:0;' wx:if='{{selectedCate=="全场直播"}}'>
        <image style='width:730rpx;padding:0;margin:0;' mode='widthFix' lazy-load src='http://cos.qkmai.com/qkmbb/ytal/remai.png'></image>
    </view>





    <view style='padding:0 0 27rpx 0;width:730rpx;margin-left:10rpx;'>
        <view wx:key="{{item}}" wx:for='{{brandRush}}' wx:for-index="bindex" style='padding-top:15rpx;margin-bottom:20rpx;background:#fff;padding-right:27rpx;border-radius:20rpx;'>
            <view style='padding:27rpx 26rpx 16rpx 16rpx;float:left;width:85rpx;height:90rpx;' bindtap='linkToBrandRush' data-brandId='{{item.brandId}}' data-brandsource='{{item.brandSource}}'>
                <view class='fcc' style='border: 1rpx solid #e5e5e5;width:85rpx;height:85rpx;'>
                    <image src='{{item.brandLogo}}' style='width:98%' mode='widthFix' lazy-load></image>
                </view>
                <view wx:if='{{item.goodsCount}}' style='width:98%;font-size:12px;text-align:center;padding-top:10rpx;font-size:26rpx;color:#db3c40;'>{{item.goodsCount? item.goodsCount : 0}}
                    <text style="font-size:22rpx;color:#959595;">款</text>
                </view>
                <view wx:else style='display:none;width:98%;font-size:12px;text-align:center;padding-top:10rpx;font-size:26rpx;color:#db3c40;'>
                    <text style="font-size:22rpx;color:#959595;">预告</text>
                </view>
            </view>
            <view style='float:right:623rpx;position:relative;'>
