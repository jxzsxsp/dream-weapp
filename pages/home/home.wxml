                    <view style='font-size:14px;text-align:center;line-height:50rpx;color:#fff;'>{{filter.toFix((item.lowerDiscount / 10),1)}}<text style='font-size:9px;'>折起</text>
                    </view>
                </view>
                <view style='height:85rpx;padding:37rpx 0 16rpx 0' bindtap='linkToBrandRush' data-brandId='{{item.brandId}}' data-brandsource='{{item.brandSource}}'>
                    <view style='font-size:30rpx;'>{{item.mainTitle}}</view>
                    <view>
                        <view>
                            <view wx:if='{{item.subTitle}}' style='font-size:20rpx;line-height:34rpx;color:#ff0000;background-color: #ffe0e7;border-radius: 15px;height:34rpx;margin-top:10rpx;height:35rpx;padding: 0 8rpx;float:left;margin-right:10rpx;'>{{item.subTitle}}</view>

                            <!-- <view style='font-size:20rpx;line-height:34rpx;color:#ff0000;background-color: #ffe0e7;	border-radius: 15px;height:34rpx;margin-top:10rpx;height:35rpx;padding: 0 8rpx;float:left'>{{filter.toFix((item.lowerDiscount / 10),1)}}折起</view> -->




                            <view style='float:right;font-size:26rpx;padding-right:15rpx' wx:if="{{userInfo.IsReferral}}">最高分佣
                                <text style='cursor:pointer;padding-left:5rpx;color:#ff0000;'>{{item.topCommissionRate}}%</text>
                            </view>
                        </view>
                    </view>
                </view>
                <view style='clear:both'></view>
                <view style='padding-left:129rpx;' bindtap='linkToBrandRush' data-brandId='{{item.brandId}}' data-brandsource='{{item.brandSource}}'>
                    <view style='font-size:28rpx;color:#515151;padding-bottom:16rpx;'>
                        <text bindlongpress='copy' data-length='item.brandDesc.length' class='{{toggleText? "nowrap" : ""}} {{item.brandDesc.length >65 ? "ellipsis" : ""}}' data-val="{{item.brandDesc}}">{{item.brandDesc}}</text>
                        <text catchtap='toggleHide' data-id='{{item.brandId}}' style='color:#db3c40;' wx:if='{{item.brandDesc.length > 65}}'>{{toggleText? "收起" : "全文"}}</text>
                    </view>
                </view>
                <view style='padding-left:109rpx'>
                    <view class='{{item.goodsImages.length == 4 ? "fourPic" : ""}}' wx:key="{{item}}" wx:for='{{item.goodsImages}}' wx:for-item='goodsImg' style='float:left;padding-right:10rpx'>
                        <image class='ninePic' src='{{goodsImg}}' bindtap='previewImg' data-imgsrc='{{goodsImg}}' data-imgs='{{item.goodsImages}}' lazy-load></image>
                    </view>
                    <view style='clear:both'></view>
                </view>




                <view style='padding-left:119rpx;padding-right:10rpx;padding-bottom:20rpx;padding:10rpx 10rpx 10rpx 119rpx;'>
                    <view style='font-size: 26rpx;	color: #b0b0b0;	line-height: 40rpx;display:flex;flex-flow:row nowrap;;'>
                        <view style='font-size:22rpx;color:#515151;width:50%;' bindtap='linkToBrandRush' data-brandId='{{item.brandId}}' data-brandsource='{{item.brandSource}}'>
                            {{item.endTimeInfo}}
                            <text style=''>结束</text>
                        </view>

                        <view style='font-size:14px;width:50%;text-align:right;color:#515151;line-height:40rpx;display:flex;flex-flow:row nowrap;justify-content:flex-end;align-items:center;' bindtap='changeFocus' data-index="{{bindex}}" data-title="{{item.mainTitle}}">
                            <image src='{{item.isFocus && item.isFocus == true? "https://qkmai-1257905846.cos.ap-shanghai.myqcloud.com/qkmbb/ytal/guanzhutb1.png" : "https://qkmai-1257905846.cos.ap-shanghai.myqcloud.com/qkmbb/ytal/guanzhutb.png"}}' style='width:40rpx;margin-right:10rpx;' mode='widthFix'></image>
                            {{item.isFocus && item.isFocus == true? "取消关注" : "关注"}}
                        </view>
                    </view>

                </view>











                <view style='height:90rpx;line-height:90rpx;display:flex;flex-flow:row nowrap;justify-content:flex-end;align-items:flex-start;width:730rpx;margin-top:35rpx;'>

                    <!-- <view style=''> -->
                    <view style='width:40%;text-align:center;height:90rpx;line-height:90rpx;background-color:#f7f7f7;position:relative;top:-4rpx;border-bottom-left-radius:20rpx;' class='tui-countdown-content'>
                        距结束
                        <text class='tui-conutdown-box tui-countdown-bg'>{{item.countDownTime.day}}</text>天
                        <text class='tui-conutdown-box'>{{item.countDownTime.hou}}</text>时
                    </view>
                    <!-- </view> -->
                    <view style='position:relative;width:30%;height:90rpx;background-color:#fff;color:#fff;font-size:30rpx;text-align:center;line-height:90rpx;background:linear-gradient(rgba(255,176,128,1), rgba(255,131,64,1));'>
                        <button style='width:100%;font-size:30rpx;line-height:90rpx;border:none;color:#fff;background:linear-gradient(rgba(255,176,128,1), rgba(255,131,64,1));' data-brandId='{{item.brandId}}' data-brandsource='{{item.brandSource}}' data-maintitle='{{item.mainTitle}}'
                            data-lower='{{item.lowerDiscount}}' data-bg='{{item.goodsImages[0]}}' plain='true' openType="share">
                            <view style='position:relative;left:25rpx;'>分享专场</view>
                        </button>
                        <image src='http://cos.qkmai.com/qkmbb/ytal/fenxiangtb.png' style='width:40rpx;position:absolute;left:35rpx;top:25rpx;' mode='widthFix'></image>
                        <!-- <view style='clear:both'></view> -->
                    </view>

                    <view style='position:relative;width:30%;height:90rpx;color:#fff;font-size:30rpx;text-align:center;line-height:90rpx;background:linear-gradient(rgba(255,108,38,1), rgba(222,34,0,1));border-bottom-right-radius:20rpx;' bindtap='linkToBrandRush' data-brandId='{{item.brandId}}'
                        data-brandsource='{{item.brandSource}}'>
                        <view style='position:relative;left:20rpx;'>去抢购</view>
                        <image src='http://cos.qkmai.com/qkmbb/ytal/qianggoutb.png' style='width:40rpx;position:absolute;left:40rpx;top:25rpx;' mode='widthFix'></image>
                    </view>
                    <!-- <view style='clear:both'></view> -->
                </view>












