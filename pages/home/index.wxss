.home-container {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 750rpx;
}

.logo {
  width: 318rpx;
  height: 321rpx;
}

.common-btn {
  width: 660rpx;
  height: 98rpx;
  border-radius: 49px;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.inbound-btn {
  background-image: linear-gradient(-134deg, #F5515F 0%, #CF0524 100%);
}

.collection-btn {
  background-image: linear-gradient(45deg, #7140F0 0%, #515FEE 100%);
}

.outbound-btn {
  background-image: linear-gradient(-90deg, #3ED9F7 0%, #0F8DF7 100%);
}