}

.videoLed {
    animation: video 15s linear infinite;
}

@keyframes video {
    0% {
        transform: translate3d(0, 0, 0);
    }

    49.8% {
        transform: translate3d(-750rpx, 0, 0);
    }

    49.9% {
        transform: translate3d(-7500rpx, 0, 0);
    }

    50% {
        transform: translate3d(7500rpx, 0, 0);
    }

    50.1% {
        transform: translate3d(750rpx, 0, 0);
    }

    100% {
        transform: translate3d(0, 0, 0);
    }
}

.ninePic {
    width: 176rpx;
    height: 176rpx;
}

.fourPic:nth-child(2) {
    width: 279rpx;
}

.ellipsis {
    display: -webkit-box;
    font-size: 28rpx;
    color: #000;
    line-height: 40rpx;
    word-break: break-all;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 10;
    overflow: hidden;
    text-overflow: ellipsis;
}

.ellipsis.nowrap {
    -webkit-line-clamp: 20;
    overflow: visible;
}

.good_box {
    width: 40px;
    height: 40px;
    position: fixed;
    border-radius: 50%;
    overflow: hidden;
    left: 50%;
    top: 50%;
    z-index: +99;
    border: 1px solid rgba(202, 202, 202, 1);
    background: rgba(202, 202, 202, 0.2);
}

.good_box image {
    display: block;
    width: 100%;
    height: 100%;
}
