.addCart {
    height:50rpx;
    width:162rpx;
    background-color: #ebebeb;
    color:#515151;
    border-radius:30rpx;
    text-align:center;
    float:right;
    line-height:50rpx;
    font-size:26rpx;
}
 .addCart.active {
     background:#ff0000;
     color:#fff;
 }

 .videoLed {
    animation: video 15s linear infinite;
 }

 @keyframes video {
    0% {
        transform: translate3d(0, 0, 0)
    }
    49.8% {
        transform: translate3d(-750rpx, 0, 0)
    }
    49.9% {
        transform: translate3d(-7500rpx, 0, 0)
    }
    50% {
       transform: translate3d(7500rpx, 0, 0)
    }
    50.1% {
        transform: translate3d(750rpx, 0, 0)
    }
    100% {
        transform: translate3d(0, 0, 0)
    }
}

.ninePic {
    width: 176rpx;
    height: 186rpx;
}
.fourPic:nth-child(2) {
    width:279rpx;
}