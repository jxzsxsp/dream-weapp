page {
    background: #f5f5f5;
    width:750rpx;
    margin:0;
    padding:0;
    box-sizing: border-box;
    max-width:750rpx;
    /* overflow: hidden; */
}

.page-section {
    background: #f5f5f5;
}

.bg-white {
    background: #fff;
}

.skuBtn {
    margin: 8rpx 8rpx 0 0;    
    background-color: #ebebeb;
    color:#515151;
}

.skuBtn.active {
    background-color: #515151;
    color: #fff;
}

.tui-countdown-content {
    height: 26rpx;
    line-height: 26rpx;
    text-align: center;
    margin: 5rpx 0 5rpx 0;
    padding: 0;
    font-size: 26rpx;
}

.tui-conutdown-box {
    display: inline-block;
    height: 26rpx;
    width: 30rpx;
    line-height: 26rpx;
    text-align: center;
    background-color: #000;
    color: #fff;
    margin: 0 5rpx;
    padding: 5rpx;
}

.tui-countdown-bg {
    background-color: #df0101;
}

.salePrice {
    font-size: 40rpx;
    line-height: 40rpx;
    color: #f00;
    padding-right: 11rpx;
}

.marketPrice {
    font-size: 24rpx;
    line-height: 24rpx;
    color: #959595;
    text-decoration: line-through;
}

.commissionMoney {
    color: #f00;
    font-size: 26rpx;
}





