.bottom-line {
  height: 22rpx;
}

.no-container {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.logo {
  margin-top: 440rpx;
  width: 120rpx;
  height: 120rpx;
}

.logo image {
  width: 100%;
  height: 100%;
}

.title {
  margin-top: 5rpx;
  font-family: PingFangSC-Medium;
  font-size: 28rpx;
  color: #C9C8D0;
  letter-spacing: 1rpx;
  text-align: center;
  line-height: 40rpx;
}

.btn {
  margin-top: 20rpx;
  width: 214rpx;
  height: 68rpx;
  border: 1rpx solid #616161;
  border-radius: 8rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-family: PingFangSC-Medium;
  font-size: 32rpx;
  color: #616161;
}