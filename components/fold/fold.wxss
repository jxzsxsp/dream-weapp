@import '../common/iconfont.wxss';

.container {
  width: 100%;
}

.head {
  width: 100%;
  height: 88rpx;
  background: #FFFFFF;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.icon {
  margin-left: 30rpx;
  color: #0055f8;
}

.show {
  transform: rotate(90deg);
}

.title {
  margin-left: 30rpx;
}

.line {
  width: 100%;
  height: 1px;
  background: #f1f1f1;
}

.content-container {
  width: 100%;
  min-height: 100rpx;
}